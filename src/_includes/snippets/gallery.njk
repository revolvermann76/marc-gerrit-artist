<link rel="stylesheet" href="{{'/assets/css/gallery.css' | url}}" />
<div class="gallery">
{% for work in collections.artworks %}
<a href="{{ work.url | url }}"> 
    <img src="{{ '/thumbnails/' | append: work.data.image | url }}" alt="{{ work.data.title }}"/>
    <p>{{ work.data.title }}</p>
</a>
{% endfor %}
</div>